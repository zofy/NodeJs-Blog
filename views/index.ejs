<% include ./partials/header%>
<div class='ui container main'>
    <% blogs.forEach(function(blog){ %>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>">
            <span>Created <%= blog.dateOfCreation %> </span>
            <p><%= blog.body %></p>
        </div>
</div>
<% }); %>
<% include ./partials/footer %>